    "amqplib": "^0.10.1",
    "axios": "^0.27.2",
    "cors": "^2.8.5",
    "debug": "^4.3.4",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "express-winston": "^4.2.0",
    "helmet": "^5.1.1",
    "mongoose": "^6.5.0",
    "uuid": "^8.3.2",
    "winston": "^3.8.1"
  },
  "devDependencies": {
    "@types/amqplib": "^0.8.2",
    "@types/cors": "^2.8.12",
    "@types/debug": "^4.1.7",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.13",
    "@types/helmet": "^4.0.0",
    "@types/node": "^18.6.2",
    "@types/uuid": "^8.3.4",
    "source-map-support": "^0.5.21",
    "ts-node-dev": "^2.0.0",
